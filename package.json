{
  "name": "kstrich_template",
  "version": "0.0.1",
  "description": "k-strich template with Babel, SASS and lodash",
  "author": "Toni Schoenbuchner",
  "license": "MIT",
  "homepage": "https://github.com/t-book/k-strich",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --progress --mode development --config webpack.config.dev.js",
    "build": "webpack -p --progress --mode production --config webpack.config.build.js",
    "deploy": "webpack -p --progress --mode production --config webpack.config.build.js && cd ../.. && ./production.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/t-book/k-strich"
  },
  "dependencies": {
    "copy-webpack-plugin": "^4.5.3",
    "hamburgers": "^1.1.3",
    "jquery": "^3.3.1",
    "owl.carousel": "^2.3.4"
  },
  "devDependencies": {
    "@babel/core": "7.1.0",
    "@babel/preset-env": "^7.1.0",
    "babel-loader": "8.0.2",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "clean-webpack-plugin": "0.1.19",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "cssnano": "^4.1.10",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "node-sass": "7.0.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "url-loader": "1.1.1",
    "webpack": "4.19.1",
    "webpack-cli": "3.1.1",
    "webpack-dev-server": "3.1.8",
    "webpack-merge": "4.1.4"
  }
}
